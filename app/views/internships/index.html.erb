<p id="notice"><%= notice %></p>

<h1>Internships</h1>

<table>
  <thead>
    <tr>
      <th>First name</th>
      <th>Last name</th>
      <th>Email</th>
      <th>Phone</th>
      <th>Gender</th>
      <th>College</th>
      <th>Graduation year</th>
      <th>Cv</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @internships.each do |internship| %>
      <tr>
        <td><%= internship.first_name %></td>
        <td><%= internship.last_name %></td>
        <td><%= internship.email %></td>
        <td><%= internship.phone %></td>
        <td><%= internship.gender %></td>
        <td><%= internship.college %></td>
        <td><%= internship.graduation_year %></td>
        <td><%= link_to "Curricurum vitae", internship.cv %></td>
        <td><%= link_to 'Show', internship %></td>
        <td><%= link_to 'Edit', edit_internship_path(internship) %></td>
        <td><%= link_to 'Destroy', internship, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Internship', new_internship_path %>
